{
  "name": "braind",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": "BrainD Team",
  "description": "Retro Pixel Art Brain Training Platform on Starknet",
  "repository": {
    "type": "git",
    "url": "https://github.com/0xOucan/Braind.git"
  },
  "workspaces": {
    "packages": [
      "packages/snfoundry",
      "packages/nextjs"
    ]
  },
  "scripts": {
    "chain": "yarn workspace @ss-2/snfoundry chain",
    "clean": "yarn workspace @ss-2/snfoundry clean",
    "compile": "yarn workspace @ss-2/snfoundry compile",
    "deploy": "yarn workspace @ss-2/snfoundry deploy",
    "deploy:clear": "yarn workspace @ss-2/snfoundry deploy:clear",
    "deploy:no-reset": "yarn workspace @ss-2/snfoundry deploy --no-reset",
    "format": "yarn workspace @braind/nextjs format && yarn workspace @ss-2/snfoundry format",
    "format:check": "yarn workspace @braind/nextjs format:check && yarn workspace @ss-2/snfoundry format:check",
    "next:check-types": "yarn workspace @braind/nextjs check-types",
    "next:lint": "yarn workspace @braind/nextjs lint",
    "prepare": "husky",
    "start": "yarn workspace @braind/nextjs dev",
    "dev": "yarn workspace @braind/nextjs dev",
    "test": "yarn workspace @ss-2/snfoundry test",
    "test:nextjs": "yarn workspace @braind/nextjs test",
    "vercel": "yarn workspace @braind/nextjs vercel",
    "vercel:yolo": "yarn workspace @braind/nextjs vercel:yolo",
    "verify": "yarn workspace @ss-2/snfoundry verify"
  },
  "dependencies": {
    "postcss": "^8.4.38",
    "vercel": "latest"
  },
  "devDependencies": {
    "daisyui": "^4.7.3",
    "husky": "^9.0.11",
    "pinst": "^3.0.0",
    "prettier": "^3.2.5"
  },
  "packageManager": "yarn@3.2.3",
  "overrides": {
    "chalk": "5.3.0",
    "strip-ansi": "7.1.0",
    "color-convert": "2.0.1",
    "color-name": "1.1.4",
    "is-core-module": "2.13.1",
    "error-ex": "1.3.2",
    "has-ansi": "5.0.1"
  }
}
